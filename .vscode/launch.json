{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Pixel Editor",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/electron/dist/electron.exe",
            "program": "${workspaceRoot}/code/index.js",
            "args": [
                "--disable-gpu-async-worker-context"
            ]
        }
    ]
}